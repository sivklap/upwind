{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AllowReadOnlyAccess",
        "Effect": "Allow",
        "Action": [
          "s3:ListBucket",
          "s3:GetObject"
        ],
        "Resource": [
          "arn:aws:s3:::example-bucket",
          "arn:aws:s3:::example-bucket/*"
        ]
      }
    ]
  }
  